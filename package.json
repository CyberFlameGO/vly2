{
  "name": "vly2",
  "version": "1.0.0",
  "description": "Voluntari.ly Client based on Next.js",
  "main": "server/server.js",
  "scripts": {
    "test": "echo 'Running Standard.js and Jasmine unit tests...\n' && yarn lint && yarn unit",
    "unit": "jasmine",
    "lint": "standard",
    "fix": "standard --fix",
    "dev": "nodemon -w server -w package.json server/server.js",
    "build": "next build && node ./scripts/default-lang",
    "heroku-postbuild": "next build",
    "start": "NODE_ENV=production node server/server.js"
  },
  "now": {
    "name": "voluntarily-vly2",
    "alias": "voluntarily"
  },
  "dependencies": {
    "@zeit/next-less": "^1.0.1",
    "accepts": "^1.3.5",
    "antd": "^3.5.4",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-react-intl": "^3.0.1",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "hoist-non-react-statics": "^3.3.0",
    "intl": "^1.2.5",
    "less": "^3.9.0",
    "less-vars-to-js": "1.3.0",
    "lodash": "^4.17.11",
    "markdown-to-jsx": "^6.9.4",
    "mongoose": "^5.5.4",
    "mongoose-crudify": "^0.2.0",
    "next": "^8.1.0",
    "next-redux-wrapper": "^3.0.0-alpha.2",
    "next-routes": "^1.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.8.0",
    "react-redux": "^7.0.2",
    "redux": "^4.0.1",
    "redux-api": "^0.11.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "jasmine": "^3.4.0",
    "nodemon": "^1.18.11",
    "standard": "^12.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voluntarily/vly2.git"
  },
  "author": "Andrew Watkins",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/voluntarily/vly2/issues"
  },
  "homepage": "https://github.com/voluntarily/vly2#readme"
}
